<mujoco model="spinner">
  <compiler angle="radian"/>

  <default>
    <default class="visual">
      <geom contype="0" conaffinity="0"/>
    </default>  
  </default>

  <!-- Converted from URDF -->
  <worldbody>
    <body name="base">
      <geom class="visual" type="sphere" size="0.07" rgba="0 0.9 0 1"/>
      <body name="finger_one" quat="0.7 0.0 0.7 0.0">
        <inertial pos="0 0 0.5" mass="1" diaginertia="0.0839 0.0839 0.00125"/>
        <joint name="theta_one" pos="0 0 0" axis="1 0 0" damping="0.1"/>
        <geom class="visual" pos="0 0 0.5" type="capsule" size="0.05 0.45" rgba="0.9 0 0 1"/>
        <body name="finger_two" pos="0 0 1">
          <inertial pos="0 0 0.5" mass="1" diaginertia="0.0839 0.0839 0.00125"/>
          <joint name="theta_two" pos="0 0 0" axis="1 0 0" damping="0.1"/>
          <geom pos="0 0 0.5" type="capsule" size="0.05 0.45" rgba="0.9 0 0 1"/>
        </body>
      </body>
    </body>
    <body name="spinner" pos="1.0 -1.5 0">
      <inertial pos="0 0 0" quat="0.7 0 0.7 0" mass="1" diaginertia="0.074 0.054 0.022"/>
      <joint name="theta_three" pos="0 0 0" axis="0 0 1" damping="0.1"/>
      <geom size="0.25" rgba="0.1 0.3 0.5 1"/>
      <geom class="visual" type="cylinder" size="0.04 0.25" rgba="0 0.9 0 1" quat="0.5 0 0.9 0"/>
    </body>
  </worldbody>

  <actuator>
    <motor joint="theta_one"/>
    <motor joint="theta_two"/>
  </actuator>

  <!-- Scene and lighting -->
  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
    <rgba haze="0.15 0.25 0.35 1"/>
  </visual>
  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
  </asset>
  <worldbody>
    <light pos="0 0 1"/>
    <geom class="visual" name="floor" pos="0 0 -0.2" size="0 0 .125" type="plane" material="groundplane"/>
  </worldbody>
</mujoco>
